{
  "cli": {
    "version": ">= 7.8.6",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "image": "latest",
        "simulator": true,
        "buildConfiguration": "Debug"
      },
      "env": {
        "NODE_ENV": "development",
        "EXPO_NO_METRO": "false"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "image": "latest"
      },
      "env": {
        "NODE_ENV": "production"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "developmentClient": false,
      "ios": {
        "image": "latest",
        "resourceClass": "m-medium"
      },
      "env": {
        "NODE_ENV": "production",
        "EXPO_NO_DEV_CLIENT": "true",
        "EXPO_SKIP_SOURCEMAP_UPLOAD": "true"
      },
      "cache": {
        "disabled": false
      }
    },
    "local": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "withoutCredentials": true
      },
      "ios": {
        "image": "latest",
        "simulator": true,
        "buildConfiguration": "Debug"
      },
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
