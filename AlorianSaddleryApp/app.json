        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.aloriansaddlery.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "132f29df-ef92-427c-b244-6a7ab3c4d48a"
      }
    },
    "platforms": [
      "ios",
      "android"
    ],
    "jsEngine": "hermes"
